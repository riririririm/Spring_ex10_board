<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="ThumbnailMapper">
	<insert id="setWrite" parameterType="ThumbnailVO">
		insert into Thumbnail values(product_seq.nextval,#{pid},#{fname},#{oname})
	</insert>
</mapper>